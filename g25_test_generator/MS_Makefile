# Sample makefile

!include <win32.mak>

all: test.exe

e3ga.g: e3ga_c_3SAxGA_multivector_scalar.xml
  g25 e3ga_c_3SAxGA_multivector_scalar.xml

e3gaParser.c: e3ga.g
  java org.antlr.Tool e3ga.g
  
.c.obj:
  $(cc) $(cdebug) $(cflags) $(cvars) -Ic:\include $*.c

test.exe: e3gaParser.obj e3gaLexer.obj e3ga.obj e3ga_parse_multivector.obj e3ga_test_main.obj 
  $(link) $(ldebug) $(conflags) /libpath:c:\lib32 -out:test.exe e3ga.obj e3ga_parse_multivector.obj e3ga_test_main.obj e3gaParser.obj e3gaLexer.obj $(conlibs) antlr3c.lib


